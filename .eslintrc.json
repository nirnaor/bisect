{
	"extends": ["eslint:recommended", "google"],
	"env": {
		"es6" : true,
		"node" : true
	},
	"rules": {
        "semi": [2, "never"]
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "script",
		"ecmaFeatures": {
			"arrowFunctions": true,
			"binaryLiterals": true,
			"blockBindings": true,
			"classes": true
		}
	}
}
